https://www.codewars.com/kata/57faa6ff9610ce181b000028/train/javascript

function crap(x, bags, cap){
let count=0;
  for (let row of x) {
    for (let el of row){
      if (el==='@'){
        count++;
      }
      if (el==='D') return 'Dog!!'
    }
  }
  return count<=bags*cap ? 'Clean': 'Cr@p';
}
